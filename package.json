{
  "name": "@tim-smart/actualbudget-sync",
  "version": "0.2.5",
  "description": "CLI for syncing banks with Actual Budget",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "packageManager": "pnpm@9.12.0",
  "repository": {
    "url": "https://github.com/tim-smart/",
    "type": "git"
  },
  "scripts": {
    "test": "vitest",
    "codegen": "build-utils prepare-v2",
    "clean": "rimraf rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "check": "tsc --noEmit",
    "changeset-version": "changeset version",
    "changeset-publish": "pnpm build && changeset publish"
  },
  "keywords": [],
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@effect/cli": "^0.49.4",
    "@effect/language-service": "^0.2.0",
    "@effect/platform": "^0.70.4",
    "@effect/platform-node": "^0.65.4",
    "@effect/vitest": "^0.14.3",
    "@types/node": "^22.10.1",
    "effect": "^3.11.3",
    "glob": "^11.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@actual-app/api": "^24.11.0",
    "playwright": "^1.49.0"
  }
}
